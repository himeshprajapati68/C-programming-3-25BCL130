#include <stdio.h>

int main() {
    int numbers[10];
    int i;

    // Accept 10 values from the user
    printf("Enter 10 numbers:\n");
    for(i = 0; i < 10; i++) {
        printf("Enter number %d: ", i + 1);
        scanf("%d", &numbers[i]);
    }

    // Print 4th, 7th, and 9th values
    printf("\n4th value: %d\n", numbers[3]);
    printf("7th value: %d\n", numbers[6]);
    printf("9th value: %d\n", numbers[8]);

    return 0;
}
