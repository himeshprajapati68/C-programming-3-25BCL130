#include <stdio.h>

int main() {
    int numbers[5];
    int i;

    // Accept 5 values from the user
    printf("Enter 5 numbers:\n");
    for(i = 0; i < 5; i++) {
        printf("Enter number %d: ", i + 1);
        scanf("%d", &numbers[i]);
    }

    // Print the 5 values
    printf("\nYou entered:\n");
    for(i = 0; i < 5; i++) {
        printf("%d ", numbers[i]);
    }

    return 0;
}
